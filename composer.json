{
    "name": "goblindegook/syllables",
    "description": "Syllables are helper classes and functions for WordPress development.",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "library", "plugin", "toolkit"],
    "homepage": "https://github.com/goblindegook/Syllables",
    "license": "GPL-2.0+",
    "authors": [
      {
          "name": "LuÃ­s Rodrigues",
          "email": "hello@goblindegook.net",
          "homepage": "https://github.com/goblindegook"
      }
    ],
    "support": {
        "issues": "https://github.com/goblindegook/Syllables/issues",
        "source": "https://github.com/goblindegook/Syllables"
    },
    "autoload": {
        "psr-4": {
            "Syllables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syllables\\Tests\\": "tests/"
        },
        "classmap": ["tests/mocks/"]
    },
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "~1.0"
    },
    "suggest": {
      "monolog/monolog": "~1.11.0",
      "wp-papi/papi": "~1.0.3",
      "jarednova/timber": "~0.20.9",
      "markjaquith/wp-tlc-transients": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*@stable",
        "apigen/apigen": "4.0.0-RC4",
        "10up/wp_mock": "dev-master"
    },
    "suggest": {}
}
