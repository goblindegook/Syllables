<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>Syllables</title>
  <parser>
    <target>docs/phpdoc</target>
  </parser>
  <transformer>
    <target>docs/phpdoc</target>
  </transformer>
  <files>
    <directory>src</directory>
    <ignore>index.php</ignore>
  </files>
  <transformation>
    <template name="checkstyle"/>
  </transformation>
</phpdoc>
